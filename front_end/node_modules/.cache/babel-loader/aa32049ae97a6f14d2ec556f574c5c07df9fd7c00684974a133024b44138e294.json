{"ast":null,"code":"import { ref, onMounted, nextTick } from \"vue\";\nimport { getRanderingSorted } from \"@/api/rendering\";\nimport { Button, Menu as aMenu } from \"ant-design-vue\";\nexport default {\n  name: \"MapWindow\",\n  components: {\n    Button,\n    aMenu\n  },\n  setup() {\n    const map = ref(null);\n    const polygons = ref([]);\n    const collapsed = ref(false);\n\n    // 切换菜单折叠\n    const toggleCollapsed = () => {\n      collapsed.value = !collapsed.value;\n    };\n\n    // 加载并初始化地图\n    const loadAMapApi = () => {\n      const script = document.createElement(\"script\");\n      script.type = \"text/javascript\";\n      script.src = \"https://webapi.amap.com/maps?v=2.0&key=a6042dfaa8137c1bd9f59034cd3021a3\";\n      document.head.appendChild(script);\n      script.onload = () => {\n        if (window.AMap) {\n          initMap(); // 确保 AMap 已经加载完毕后再初始化地图\n        } else {\n          console.error(\"AMap API failed to load.\");\n        }\n      };\n    };\n    const initMap = () => {\n      const layer = new AMap.createDefaultLayer({\n        zooms: [3, 20],\n        visible: true,\n        opacity: 1,\n        zIndex: 0\n      });\n      map.value = new AMap.Map(\"container\", {\n        viewMode: \"2D\",\n        zoom: 16.3,\n        center: [113.266949, 35.188365],\n        layer: [layer]\n      });\n    };\n\n    // 获取并渲染数据\n    onMounted(async () => {\n      await nextTick(); // 确保 DOM 更新完成后执行\n      loadAMapApi(); // 加载 AMap API\n    });\n    return {\n      map,\n      polygons,\n      collapsed,\n      toggleCollapsed\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","nextTick","getRanderingSorted","Button","Menu","aMenu","name","components","setup","map","polygons","collapsed","toggleCollapsed","value","loadAMapApi","script","document","createElement","type","src","head","appendChild","onload","window","AMap","initMap","console","error","layer","createDefaultLayer","zooms","visible","opacity","zIndex","Map","viewMode","zoom","center"],"sources":["E:\\code\\Django\\project\\front\\front\\src\\components\\LocationWindow.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\" style=\"position: relative; width: 100%; height: 100%\">\r\n    <!-- 地图容器 -->\r\n    <div\r\n      id=\"container\"\r\n      style=\"width: 100%; height: 100%; position: absolute; z-index: 0\"\r\n    ></div>\r\n\r\n    <!-- 菜单部分 -->\r\n    <div\r\n      style=\"\r\n        position: absolute;\r\n        top: 20px;\r\n        left: 20px;\r\n        z-index: 10;\r\n        transition: all 0.3s ease;\r\n      \"\r\n    >\r\n      <Button\r\n        type=\"primary\"\r\n        @click=\"toggleCollapsed\"\r\n        style=\"margin-bottom: 16; z-index: 10\"\r\n      >\r\n        {{ collapsed ? \"Expand\" : \"Collapse\" }}\r\n      </Button>\r\n      <a-menu\r\n        :inline-collapsed=\"collapsed\"\r\n        mode=\"inline\"\r\n        theme=\"dark\"\r\n        :default-selected-keys=\"['1']\"\r\n        :default-open-keys=\"['sub1']\"\r\n        style=\"transition: all 0.3s ease\"\r\n      >\r\n        <a-menu-item key=\"1\" icon=\"pie-chart\"> Option 1 </a-menu-item>\r\n        <a-menu-item key=\"2\" icon=\"desktop\"> Option 2 </a-menu-item>\r\n        <a-menu-item key=\"3\" icon=\"container\"> Option 3 </a-menu-item>\r\n        <a-sub-menu key=\"sub1\" icon=\"mail\" title=\"Navigation One\">\r\n          <a-menu-item key=\"5\">Option 5</a-menu-item>\r\n          <a-menu-item key=\"6\">Option 6</a-menu-item>\r\n          <a-menu-item key=\"7\">Option 7</a-menu-item>\r\n          <a-menu-item key=\"8\">Option 8</a-menu-item>\r\n        </a-sub-menu>\r\n        <a-sub-menu key=\"sub2\" icon=\"appstore\" title=\"Navigation Two\">\r\n          <a-menu-item key=\"9\">Option 9</a-menu-item>\r\n          <a-menu-item key=\"10\">Option 10</a-menu-item>\r\n          <a-sub-menu key=\"sub3\" title=\"Submenu\">\r\n            <a-menu-item key=\"11\">Option 11</a-menu-item>\r\n            <a-menu-item key=\"12\">Option 12</a-menu-item>\r\n          </a-sub-menu>\r\n        </a-sub-menu>\r\n      </a-menu>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, nextTick } from \"vue\";\r\nimport { getRanderingSorted } from \"@/api/rendering\";\r\nimport { Button, Menu as aMenu } from \"ant-design-vue\";\r\n\r\nexport default {\r\n  name: \"MapWindow\",\r\n  components: {\r\n    Button,\r\n    aMenu,\r\n  },\r\n  setup() {\r\n    const map = ref(null);\r\n    const polygons = ref([]);\r\n    const collapsed = ref(false);\r\n\r\n    // 切换菜单折叠\r\n    const toggleCollapsed = () => {\r\n      collapsed.value = !collapsed.value;\r\n    };\r\n\r\n    // 加载并初始化地图\r\n    const loadAMapApi = () => {\r\n      const script = document.createElement(\"script\");\r\n      script.type = \"text/javascript\";\r\n      script.src =\r\n        \"https://webapi.amap.com/maps?v=2.0&key=a6042dfaa8137c1bd9f59034cd3021a3\";\r\n      document.head.appendChild(script);\r\n\r\n      script.onload = () => {\r\n        if (window.AMap) {\r\n          initMap(); // 确保 AMap 已经加载完毕后再初始化地图\r\n        } else {\r\n          console.error(\"AMap API failed to load.\");\r\n        }\r\n      };\r\n    };\r\n\r\n    const initMap = () => {\r\n      const layer = new AMap.createDefaultLayer({\r\n        zooms: [3, 20],\r\n        visible: true,\r\n        opacity: 1,\r\n        zIndex: 0,\r\n      });\r\n\r\n      map.value = new AMap.Map(\"container\", {\r\n        viewMode: \"2D\",\r\n        zoom: 16.3,\r\n        center: [113.266949, 35.188365],\r\n        layer: [layer],\r\n      });\r\n    };\r\n\r\n    // 获取并渲染数据\r\n    onMounted(async () => {\r\n      await nextTick(); // 确保 DOM 更新完成后执行\r\n      loadAMapApi(); // 加载 AMap API\r\n    });\r\n\r\n    return {\r\n      map,\r\n      polygons,\r\n      collapsed,\r\n      toggleCollapsed,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\nhtml,\r\nbody,\r\n#app {\r\n  width: 100%;\r\n  height: 100%;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n#container {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 0; /* 确保地图位于底层 */\r\n}\r\n\r\n.ant-menu {\r\n  position: absolute;\r\n  top: 20px;\r\n  left: 20px;\r\n  z-index: 10; /* 确保菜单位于地图之上 */\r\n}\r\n\r\n.amap-logo {\r\n  display: none !important;\r\n}\r\n\r\n.amap-copyright {\r\n  opacity: 0 !important;\r\n}\r\n</style>\r\n"],"mappings":"AAwDA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAC9C,SAASC,kBAAiB,QAAS,iBAAiB;AACpD,SAASC,MAAM,EAAEC,IAAG,IAAKC,KAAI,QAAS,gBAAgB;AAEtD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVJ,MAAM;IACNE;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,GAAE,GAAIV,GAAG,CAAC,IAAI,CAAC;IACrB,MAAMW,QAAO,GAAIX,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMY,SAAQ,GAAIZ,GAAG,CAAC,KAAK,CAAC;;IAE5B;IACA,MAAMa,eAAc,GAAIA,CAAA,KAAM;MAC5BD,SAAS,CAACE,KAAI,GAAI,CAACF,SAAS,CAACE,KAAK;IACpC,CAAC;;IAED;IACA,MAAMC,WAAU,GAAIA,CAAA,KAAM;MACxB,MAAMC,MAAK,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ,CAAC;MAC/CF,MAAM,CAACG,IAAG,GAAI,iBAAiB;MAC/BH,MAAM,CAACI,GAAE,GACP,yEAAyE;MAC3EH,QAAQ,CAACI,IAAI,CAACC,WAAW,CAACN,MAAM,CAAC;MAEjCA,MAAM,CAACO,MAAK,GAAI,MAAM;QACpB,IAAIC,MAAM,CAACC,IAAI,EAAE;UACfC,OAAO,CAAC,CAAC,EAAE;QACb,OAAO;UACLC,OAAO,CAACC,KAAK,CAAC,0BAA0B,CAAC;QAC3C;MACF,CAAC;IACH,CAAC;IAED,MAAMF,OAAM,GAAIA,CAAA,KAAM;MACpB,MAAMG,KAAI,GAAI,IAAIJ,IAAI,CAACK,kBAAkB,CAAC;QACxCC,KAAK,EAAE,CAAC,CAAC,EAAE,EAAE,CAAC;QACdC,OAAO,EAAE,IAAI;QACbC,OAAO,EAAE,CAAC;QACVC,MAAM,EAAE;MACV,CAAC,CAAC;MAEFxB,GAAG,CAACI,KAAI,GAAI,IAAIW,IAAI,CAACU,GAAG,CAAC,WAAW,EAAE;QACpCC,QAAQ,EAAE,IAAI;QACdC,IAAI,EAAE,IAAI;QACVC,MAAM,EAAE,CAAC,UAAU,EAAE,SAAS,CAAC;QAC/BT,KAAK,EAAE,CAACA,KAAK;MACf,CAAC,CAAC;IACJ,CAAC;;IAED;IACA5B,SAAS,CAAC,YAAY;MACpB,MAAMC,QAAQ,CAAC,CAAC,EAAE;MAClBa,WAAW,CAAC,CAAC,EAAE;IACjB,CAAC,CAAC;IAEF,OAAO;MACLL,GAAG;MACHC,QAAQ;MACRC,SAAS;MACTC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}