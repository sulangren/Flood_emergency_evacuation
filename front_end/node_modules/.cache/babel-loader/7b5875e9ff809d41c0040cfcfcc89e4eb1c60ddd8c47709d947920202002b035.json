{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { Layout, Button, Modal, Form, Input, InputPassword } from \"ant-design-vue\";\nexport default {\n  components: {\n    \"a-layout\": Layout,\n    \"a-layout-header\": Layout.Header,\n    \"a-layout-content\": Layout.Content,\n    \"a-layout-Footer\": Layout.Footer,\n    \"a-row\": Layout.Row,\n    \"a-col\": Layout.Col,\n    \"a-button\": Button,\n    \"a-modal\": Modal,\n    \"a-form-item\": Form.Item,\n    \"a-input\": Input,\n    \"a-input-password\": InputPassword\n  },\n  data() {\n    return {\n      activeTab: null,\n      isModalVisible: false,\n      // 控制模态框显示与隐藏\n      form: {\n        username: \"\",\n        password: \"\",\n        remember: true\n      }\n    };\n  },\n  methods: {\n    login() {\n      const loginData = {\n        username: this.username,\n        password: this.password\n      };\n      axios.post(\"http://127.0.1:8000/\", loginData).then(response => {\n        // 处理登录成功，例如保存token\n        localStorage.setItem(\"token\", response.data.token);\n        // 跳转到主页或其他页面\n        this.$router.push(\"/home\");\n      }).catch(error => {\n        // 处理错误\n        console.error(\"登录失败\", error);\n      });\n    },\n    onFinish(values) {\n      console.log(\"Success:\", values);\n    },\n    onFinishFailed(errorInfo) {\n      console.log(\"Failed:\", errorInfo);\n    },\n    goToRegisterPage() {\n      // 打开新窗口或标签页的 URL，根据实际情况修改\n      this.$router.push(\"/enroll\");\n      this.isModalVisible = false; // 关闭模态框\n    },\n    setActiveTab(tabIndex) {\n      this.activeTab = tabIndex;\n      switch (tabIndex) {\n        case 1:\n          this.$router.push({\n            path: \"/\"\n          });\n          break;\n        case 2:\n          this.$router.push({\n            path: \"/data\"\n          });\n          break;\n        case 4:\n          this.$router.push({\n            path: \"/dispatch\"\n          });\n          break;\n        default:\n          this.$router.push({\n            path: \"/\"\n          });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Layout","Button","Modal","Form","Input","InputPassword","components","Header","Content","Footer","Row","Col","Item","data","activeTab","isModalVisible","form","username","password","remember","methods","login","loginData","post","then","response","localStorage","setItem","token","$router","push","catch","error","console","onFinish","values","log","onFinishFailed","errorInfo","goToRegisterPage","setActiveTab","tabIndex","path"],"sources":["E:\\code\\Django\\project\\front\\front\\src\\components\\MainWindow.vue"],"sourcesContent":["<template>\n  <a-layout class=\"layout\">\n    <a-layout-header class=\"header-one\">\n      <a-row align=\"middle\" justify=\"space-between\" class=\"header-one-layout\">\n        <a-col :span=\"2\">\n          <img src=\"../image/logo.png\" class=\"header-one-image\" />\n        </a-col>\n        <a-col :span=\"18\">\n          <div class=\"logo-text\">GIS校园共享单车调度系统</div>\n        </a-col>\n        <a-col :span=\"2\">\n          <a-button class=\"a-button-logIn\" @click=\"isModalVisible = true\"\n            >注册/登入</a-button\n          >\n        </a-col>\n        <a-col :span=\"2\">\n          <a-button class=\"a-button-help\">关于我们</a-button>\n        </a-col>\n      </a-row>\n    </a-layout-header>\n\n    <a-layout-header class=\"header-tow\">\n      <a-row align=\"middle\" justify=\"space-between\" class=\"header-one-layout\">\n        <a-col :span=\"24\" class=\"header-tow-button\">\n          <a-button\n            key=\"1\"\n            @click=\"setActiveTab(1)\"\n            class=\"a-button-Map\"\n            :class=\"{ 'active-tab': activeTab === 1 }\"\n          >\n            首页\n          </a-button>\n          <a-button\n            key=\"2\"\n            @click=\"setActiveTab(2)\"\n            class=\"a-button-Data\"\n            :class=\"{ 'active-tab': activeTab === 2 }\"\n          >\n            车辆情况统计\n          </a-button>\n          <a-button\n            key=\"3\"\n            @click=\"setActiveTab(3)\"\n            class=\"a-button-user\"\n            :class=\"{ 'active-tab': activeTab === 3 }\"\n          >\n            附近车辆查询\n          </a-button>\n          <a-button\n            key=\"4\"\n            @click=\"setActiveTab(4)\"\n            class=\"a-button-user\"\n            :class=\"{ 'active-tab': activeTab === 4 }\"\n          >\n            车辆位置调度系统\n          </a-button>\n          <a-button\n            key=\"5\"\n            @click=\"setActiveTab(5)\"\n            class=\"a-button-user\"\n            :class=\"{ 'active-tab': activeTab === 5 }\"\n          >\n            车辆电量调度系统\n          </a-button>\n        </a-col>\n      </a-row>\n    </a-layout-header>\n\n    <a-layout-content class=\"content\">\n      <router-view></router-view>\n    </a-layout-content>\n  </a-layout>\n\n  <!-- 登录/注册模态框 -->\n  <a-modal v-model:visible=\"isModalVisible\" title=\"注册\" :footer=\"null\">\n    <a-layout-content>\n      <a-form\n        :model=\"form\"\n        :label-col=\"{ span: 8 }\"\n        :wrapper-col=\"{ span: 16 }\"\n        style=\"max-width: 600px\"\n        @finish=\"onFinish\"\n        @finish-failed=\"onFinishFailed\"\n        autocomplete=\"off\"\n      >\n        <a-form-item\n          label=\"用户名\"\n          name=\"username\"\n          :rules=\"[{ required: true, message: '请输入用户名!' }]\"\n        >\n          <a-input v-model=\"form.username\" />\n        </a-form-item>\n\n        <a-form-item\n          label=\"密码\"\n          name=\"password\"\n          :rules=\"[{ required: true, message: '请输入密码!' }]\"\n        >\n          <a-input-password v-model=\"form.password\" />\n        </a-form-item>\n\n        <a-layout-Footer class=\"a-modal-button-tow\">\n          <a-form-item>\n            <a-button\n              class=\"a-model-button-logIn\"\n              type=\"primary\"\n              html-type=\"submit\"\n              @class=\"login\"\n              >登入</a-button\n            >\n          </a-form-item>\n        </a-layout-Footer>\n        <a-layout-Footer class=\"a-modal-button-one\">\n          <a-button class=\"a-model-button-enroll\" @click=\"goToRegisterPage\"\n            >点我注册</a-button\n          >\n        </a-layout-Footer>\n      </a-form>\n    </a-layout-content>\n  </a-modal>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport {\n  Layout,\n  Button,\n  Modal,\n  Form,\n  Input,\n  InputPassword,\n} from \"ant-design-vue\";\n\nexport default {\n  components: {\n    \"a-layout\": Layout,\n    \"a-layout-header\": Layout.Header,\n    \"a-layout-content\": Layout.Content,\n    \"a-layout-Footer\": Layout.Footer,\n    \"a-row\": Layout.Row,\n    \"a-col\": Layout.Col,\n    \"a-button\": Button,\n    \"a-modal\": Modal,\n    \"a-form-item\": Form.Item,\n    \"a-input\": Input,\n    \"a-input-password\": InputPassword,\n  },\n  data() {\n    return {\n      activeTab: null,\n      isModalVisible: false, // 控制模态框显示与隐藏\n      form: {\n        username: \"\",\n        password: \"\",\n        remember: true,\n      },\n    };\n  },\n  methods: {\n    login() {\n      const loginData = {\n        username: this.username,\n        password: this.password,\n      };\n      axios\n        .post(\"http://127.0.1:8000/\", loginData)\n        .then((response) => {\n          // 处理登录成功，例如保存token\n          localStorage.setItem(\"token\", response.data.token);\n          // 跳转到主页或其他页面\n          this.$router.push(\"/home\");\n        })\n        .catch((error) => {\n          // 处理错误\n          console.error(\"登录失败\", error);\n        });\n    },\n    onFinish(values) {\n      console.log(\"Success:\", values);\n    },\n    onFinishFailed(errorInfo) {\n      console.log(\"Failed:\", errorInfo);\n    },\n    goToRegisterPage() {\n      // 打开新窗口或标签页的 URL，根据实际情况修改\n      this.$router.push(\"/enroll\");\n      this.isModalVisible = false; // 关闭模态框\n    },\n    setActiveTab(tabIndex) {\n      this.activeTab = tabIndex;\n      switch (tabIndex) {\n        case 1:\n          this.$router.push({ path: \"/\" });\n          break;\n        case 2:\n          this.$router.push({ path: \"/data\" });\n          break;\n        case 4:\n          this.$router.push({ path: \"/dispatch\" });\n          break;\n        default:\n          this.$router.push({ path: \"/\" });\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.layout {\n  height: 100vh;\n  background: #f0f2f5;\n}\n\n.header-one {\n  background: #001529;\n  padding: 0 16px;\n  height: 50px;\n}\n\n.logo-text {\n  position: relative;\n  font-size: 20px; /* 根据需要调整字体大小 */\n  color: #fff; /* 文字颜色 */\n  margin-left: 10%; /* 根据需要调整左边距 */\n  top: -8px;\n}\n\n.header-one-image {\n  margin-bottom: 2px;\n  width: 60px;\n  height: 40px;\n}\n\n.a-button-logIn,\n.a-button-help {\n  position: relative;\n  border: none;\n  background: #001529;\n  color: #fff;\n  box-shadow: none; /* 去除阴影 */\n  top: -6px;\n}\n\n.header-one-dropdown {\n  margin-left: 16px;\n  margin-bottom: 15%;\n}\n\n.content {\n  padding: 16px;\n}\n\n.header-tow {\n  background: #ededed;\n  padding: 0 16px;\n  height: 50px;\n}\n\n.header-tow-button {\n  margin-top: 8px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.header-tow-button .a-button-Map,\n.header-tow-button .a-button-Data,\n.header-tow-button .a-button-user {\n  margin-left: 10px;\n  margin-right: 10px;\n  position: relative;\n  border: none;\n  background: #ededed;\n  box-shadow: none; /* 去除阴影 */\n}\n\n.active-tab::after {\n  content: \"\";\n  display: block;\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  height: 2px;\n  background: #1890ff;\n}\n\n/* 弹窗样式 */\n.a-modal-button-one {\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n}\n.a-modal-button-tow {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n.a-model-button-logIn {\n  margin-left: 10px;\n  margin-right: 10px;\n  width: 300px;\n}\n.a-model-button-enroll {\n  margin-left: 10px;\n  margin-right: 10px;\n  border: none;\n  background: #fff;\n  box-shadow: none; /* 去除阴影 */\n}\n</style>\n"],"mappings":";AA2HA,OAAOA,KAAI,MAAO,OAAO;AACzB,SACEC,MAAM,EACNC,MAAM,EACNC,KAAK,EACLC,IAAI,EACJC,KAAK,EACLC,aAAa,QACR,gBAAgB;AAEvB,eAAe;EACbC,UAAU,EAAE;IACV,UAAU,EAAEN,MAAM;IAClB,iBAAiB,EAAEA,MAAM,CAACO,MAAM;IAChC,kBAAkB,EAAEP,MAAM,CAACQ,OAAO;IAClC,iBAAiB,EAAER,MAAM,CAACS,MAAM;IAChC,OAAO,EAAET,MAAM,CAACU,GAAG;IACnB,OAAO,EAAEV,MAAM,CAACW,GAAG;IACnB,UAAU,EAAEV,MAAM;IAClB,SAAS,EAAEC,KAAK;IAChB,aAAa,EAAEC,IAAI,CAACS,IAAI;IACxB,SAAS,EAAER,KAAK;IAChB,kBAAkB,EAAEC;EACtB,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,cAAc,EAAE,KAAK;MAAE;MACvBC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,MAAMC,SAAQ,GAAI;QAChBL,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MACDnB,KAAI,CACDwB,IAAI,CAAC,sBAAsB,EAAED,SAAS,EACtCE,IAAI,CAAEC,QAAQ,IAAK;QAClB;QACAC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEF,QAAQ,CAACZ,IAAI,CAACe,KAAK,CAAC;QAClD;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;MAC5B,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChB;QACAC,OAAO,CAACD,KAAK,CAAC,MAAM,EAAEA,KAAK,CAAC;MAC9B,CAAC,CAAC;IACN,CAAC;IACDE,QAAQA,CAACC,MAAM,EAAE;MACfF,OAAO,CAACG,GAAG,CAAC,UAAU,EAAED,MAAM,CAAC;IACjC,CAAC;IACDE,cAAcA,CAACC,SAAS,EAAE;MACxBL,OAAO,CAACG,GAAG,CAAC,SAAS,EAAEE,SAAS,CAAC;IACnC,CAAC;IACDC,gBAAgBA,CAAA,EAAG;MACjB;MACA,IAAI,CAACV,OAAO,CAACC,IAAI,CAAC,SAAS,CAAC;MAC5B,IAAI,CAACf,cAAa,GAAI,KAAK,EAAE;IAC/B,CAAC;IACDyB,YAAYA,CAACC,QAAQ,EAAE;MACrB,IAAI,CAAC3B,SAAQ,GAAI2B,QAAQ;MACzB,QAAQA,QAAQ;QACd,KAAK,CAAC;UACJ,IAAI,CAACZ,OAAO,CAACC,IAAI,CAAC;YAAEY,IAAI,EAAE;UAAI,CAAC,CAAC;UAChC;QACF,KAAK,CAAC;UACJ,IAAI,CAACb,OAAO,CAACC,IAAI,CAAC;YAAEY,IAAI,EAAE;UAAQ,CAAC,CAAC;UACpC;QACF,KAAK,CAAC;UACJ,IAAI,CAACb,OAAO,CAACC,IAAI,CAAC;YAAEY,IAAI,EAAE;UAAY,CAAC,CAAC;UACxC;QACF;UACE,IAAI,CAACb,OAAO,CAACC,IAAI,CAAC;YAAEY,IAAI,EAAE;U