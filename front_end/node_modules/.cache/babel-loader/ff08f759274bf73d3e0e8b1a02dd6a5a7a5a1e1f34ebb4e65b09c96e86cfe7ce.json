{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { Layout, Button, Modal, Form, Input, Password } from \"ant-design-vue\";\nexport default {\n  components: {\n    \"a-layout\": Layout,\n    \"a-layout-header\": Layout.Header,\n    \"a-layout-content\": Layout.Content,\n    \"a-layout-footer\": Layout.Footer,\n    \"a-row\": Layout.Row,\n    \"a-col\": Layout.Col,\n    \"a-button\": Button,\n    \"a-modal\": Modal,\n    \"a-form\": Form,\n    \"a-form-item\": Form.Item,\n    \"a-input\": Input,\n    \"a-input-password\": Password\n  },\n  data() {\n    return {\n      activeTab: null,\n      isModalVisible: false,\n      form: {\n        username: \"\",\n        password: \"\"\n      }\n    };\n  },\n  methods: {\n    login() {\n      const loginData = {\n        username: this.form.username,\n        password: this.form.password\n      };\n      console.log(\"Username:\", this.form.username);\n      console.log(\"Password:\", this.form.password);\n      axios.post(\"http://127.0.0.1:8000/api/login/\", loginData).then(response => {\n        // 处理登录成功\n        localStorage.setItem(\"token\", response.data.token);\n        this.$router.push(\"/home\");\n      }).catch(error => {\n        // 处理错误\n        console.error(\"登录失败\", error);\n      });\n    },\n    goToRegisterPage() {\n      this.$router.push(\"/enroll\");\n      this.isModalVisible = false;\n    },\n    setActiveTab(tabIndex) {\n      this.activeTab = tabIndex;\n      switch (tabIndex) {\n        case 1:\n          this.$router.push({\n            path: \"/\"\n          });\n          break;\n        case 2:\n          this.$router.push({\n            path: \"/data\"\n          });\n          break;\n        case 4:\n          this.$router.push({\n            path: \"/dispatch\"\n          });\n          break;\n        default:\n          this.$router.push({\n            path: \"/\"\n          });\n      }\n    }\n  }\n};","map":{"version":3,"names":["