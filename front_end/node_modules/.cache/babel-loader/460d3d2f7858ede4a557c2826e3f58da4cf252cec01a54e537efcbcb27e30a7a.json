{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { nextTick, reactive } from \"vue\";\nimport { useAuthStore } from \"@/storage/userStorage\";\nimport { useRouter } from \"vue-router\";\nimport { Layout, Button, Modal, Form, Input, InputPassword, message } from \"ant-design-vue\";\nexport default {\n  components: {\n    \"a-layout\": Layout,\n    \"a-layout-header\": Layout.Header,\n    \"a-layout-content\": Layout.Content,\n    \"a-layout-Footer\": Layout.Footer,\n    \"a-row\": Layout.Row,\n    \"a-col\": Layout.Col,\n    \"a-button\": Button,\n    \"a-modal\": Modal,\n    \"a-form-item\": Form.Item,\n    \"a-input\": Input,\n    \"a-input-password\": InputPassword\n  },\n  data() {\n    return {\n      activeTab: null,\n      isModalVisible: false,\n      // 控制模态框显示与隐藏\n      form: reactive({\n        username: \"\",\n        password: \"\"\n      }),\n      rules: {\n        username: [{\n          required: true\n        }, {\n          min: 4,\n          max: 10,\n          message: \"请输入正确的用户名\",\n          triggger: \"blur\"\n        }],\n        password: [{\n          required: true\n        }, {\n          min: 8,\n          max: 16,\n          message: \"请输入正确的密码\",\n          triggger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    async login() {\n      await this.handleSubmit(this.form);\n    },\n    async handleSubmit(values) {\n      const authStore = useAuthStore();\n      const router = useRouter();\n      try {\n        const data = await authStore.login({\n          username: values.username,\n          password: values.password\n        });\n        authStore.user.username = data.data;\n        authStore.user.token = data.token;\n        await nextTick(); // 确保 DOM 更新完成\n        message.success(\"登录成功\");\n        await router.push({\n          path: \"/\",\n          replace: true\n        });\n      } catch (error) {\n        console.error(\"Login failed:\", error);\n        message.error(\"登录失败\");\n      }\n    },\n    onFinishFailed(errorInfo) {\n      console.log(\"Failed:\", errorInfo);\n    },\n    goToRegisterPage() {\n      // 打开新窗口或标签页的 URL，根据实际情况修改\n      this.$router.push(\"/enroll\");\n      this.isModalVisible = false; // 关闭模态框\n    },\n    setActiveTab(tabIndex) {\n      this.activeTab = tabIndex;\n      switch (tabIndex) {\n        case 1:\n          this.$router.push({\n            path: \"/\"\n          });\n          break;\n        case 2:\n          this.$router.push({\n            path: \"/data\"\n          });\n          break;\n        case 4:\n          this.$router.push({\n            path: \"/dispatch\"\n          });\n          break;\n        default:\n          this.$router.push({\n            path: \"/\"\n          });\n      }\n    }\n  }\n};","map":{"version":3,"names":["nextTick","reactive","useAuthStore","useRouter","Layout","Button","Modal","Form","Input","InputPassword","message","components","Header","Content","Footer","Row","Col","Item","data","activeTab","isModalVisible","form","username","password","rules","required","min","max","triggger","methods","login","handleSubmit","values","authStore","router","user","token","success","push","path","replace","error","console","onFinishFailed","errorInfo","log","goToRegisterPage","$router","setActiveTab","tabIndex"],"sources":["E:\\code\\Django\\project\\front\\front\\src\\components\\MainWindow.vue"],"sourcesContent":["<template>\n  <a-layout class=\"layout\">\n    <a-layout-header class=\"header-one\">\n      <a-row align=\"middle\" justify=\"space-between\" class=\"header-one-layout\">\n        <a-col :span=\"4\">\n          <img src=\"../image/logo.png\" class=\"header-one-image\" />\n        </a-col>\n        <a-col :span=\"16\">\n          <div class=\"logo-text\">GIS校园共享单车调度系统</div>\n        </a-col>\n        <a-col :span=\"2\">\n          <a-button class=\"a-button-logIn\" @click=\"isModalVisible = true\"\n            >注册/登入</a-button\n          >\n        </a-col>\n        <a-col :span=\"2\">\n          <a-button class=\"a-button-help\">关于我们</a-button>\n        </a-col>\n      </a-row>\n    </a-layout-header>\n\n    <a-layout-header class=\"header-tow\">\n      <a-row align=\"middle\" justify=\"space-between\" class=\"header-one-layout\">\n        <a-col :span=\"24\" class=\"header-tow-button\">\n          <a-button\n            key=\"1\"\n            @click=\"setActiveTab(1)\"\n            class=\"a-button-Map\"\n            :class=\"{ 'active-tab': activeTab === 1 }\"\n          >\n            首页\n          </a-button>\n          <a-button\n            key=\"2\"\n            @click=\"setActiveTab(2)\"\n            class=\"a-button-Data\"\n            :class=\"{ 'active-tab': activeTab === 2 }\"\n          >\n            车辆情况统计\n          </a-button>\n          <a-button\n            key=\"3\"\n            @click=\"setActiveTab(3)\"\n            class=\"a-button-user\"\n            :class=\"{ 'active-tab': activeTab === 3 }\"\n          >\n            附近车辆查询\n          </a-button>\n          <a-button\n            key=\"4\"\n            @click=\"setActiveTab(4)\"\n            class=\"a-button-user\"\n            :class=\"{ 'active-tab': activeTab === 4 }\"\n          >\n            车辆位置调度系统\n          </a-button>\n          <a-button\n            key=\"5\"\n            @click=\"setActiveTab(5)\"\n            class=\"a-button-user\"\n            :class=\"{ 'active-tab': activeTab === 5 }\"\n          >\n            车辆电量调度系统\n          </a-button>\n        </a-col>\n      </a-row>\n    </a-layout-header>\n\n    <a-layout-content class=\"content\">\n      <router-view></router-view>\n    </a-layout-content>\n  </a-layout>\n\n  <!-- 登录/注册模态框 -->\n  <a-modal v-model:visible=\"isModalVisible\" title=\"注册\" :footer=\"null\">\n    <a-layout-content>\n      <a-form\n        :model=\"form\"\n        :rules=\"rules\"\n        :label-col=\"{ span: 8 }\"\n        :wrapper-col=\"{ span: 16 }\"\n        style=\"max-width: 600px\"\n        @finish=\"login\"\n        @finish-failed=\"onFinishFailed\"\n      >\n        <a-form-item label=\"用户名\" name=\"username\" hasFeedback>\n          <a-input v-model:value=\"form.username\" />\n        </a-form-item>\n\n        <a-form-item label=\"密码\" name=\"password\" hasFeedback>\n          <a-input-password v-model:value=\"form.password\" />\n        </a-form-item>\n\n        <a-layout-Footer class=\"a-modal-button-tow\">\n          <a-form-item>\n            <a-button\n              class=\"a-model-button-logIn\"\n              type=\"primary\"\n              html-type=\"submit\"\n              >登入</a-button\n            >\n          </a-form-item>\n        </a-layout-Footer>\n        <a-layout-Footer class=\"a-modal-button-one\">\n          <a-button class=\"a-model-button-enroll\" @click=\"goToRegisterPage\"\n            >点我注册</a-button\n          >\n        </a-layout-Footer>\n      </a-form>\n    </a-layout-content>\n  </a-modal>\n</template>\n\n<script>\nimport { nextTick, reactive } from \"vue\";\nimport { useAuthStore } from \"@/storage/userStorage\";\nimport { useRouter } from \"vue-router\";\nimport {\n  Layout,\n  Button,\n  Modal,\n  Form,\n  Input,\n  InputPassword,\n  message,\n} from \"ant-design-vue\";\n\nexport default {\n  components: {\n    \"a-layout\": Layout,\n    \"a-layout-header\": Layout.Header,\n    \"a-layout-content\": Layout.Content,\n    \"a-layout-Footer\": Layout.Footer,\n    \"a-row\": Layout.Row,\n    \"a-col\": Layout.Col,\n    \"a-button\": Button,\n    \"a-modal\": Modal,\n    \"a-form-item\": Form.Item,\n    \"a-input\": Input,\n    \"a-input-password\": InputPassword,\n  },\n  data() {\n    return {\n      activeTab: null,\n      isModalVisible: false, // 控制模态框显示与隐藏\n      form: reactive({\n        username: \"\",\n        password: \"\",\n      }),\n      rules: {\n        username: [\n          {\n            required: true,\n          },\n          { min: 4, max: 10, message: \"请输入正确的用户名\", triggger: \"blur\" },\n        ],\n        password: [\n          {\n            required: true,\n          },\n          { min: 8, max: 16, message: \"请输入正确的密码\", triggger: \"blur\" },\n        ],\n      },\n    };\n  },\n  methods: {\n    async login() {\n      await this.handleSubmit(this.form);\n    },\n    async handleSubmit(values) {\n      const authStore = useAuthStore();\n      const router = useRouter();\n      try {\n        const data = await authStore.login({\n          username: values.username,\n          password: values.password,\n        });\n        authStore.user.username = data.data;\n        authStore.user.token = data.token;\n        await nextTick(); // 确保 DOM 更新完成\n        message.success(\"登录成功\");\n        await router.push({\n          path: \"/\",\n          replace: true,\n        });\n      } catch (error) {\n        console.error(\"Login failed:\", error);\n        message.error(\"登录失败\");\n      }\n    },\n    onFinishFailed(errorInfo) {\n      console.log(\"Failed:\", errorInfo);\n    },\n    goToRegisterPage() {\n      // 打开新窗口或标签页的 URL，根据实际情况修改\n      this.$router.push(\"/enroll\");\n      this.isModalVisible = false; // 关闭模态框\n    },\n    setActiveTab(tabIndex) {\n      this.activeTab = tabIndex;\n      switch (tabIndex) {\n        case 1:\n          this.$router.push({ path: \"/\" });\n          break;\n        case 2:\n          this.$router.push({ path: \"/data\" });\n          break;\n        case 4:\n          this.$router.push({ path: \"/dispatch\" });\n          break;\n        default:\n          this.$router.push({ path: \"/\" });\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.layout {\n  height: 100vh;\n  background: #f0f2f5;\n}\n\n.header-one {\n  background: #001529;\n  padding: 0 16px;\n  height: 50px;\n}\n\n.logo-text {\n  position: relative;\n  font-size: 20px; /* 根据需要调整字体大小 */\n  color: #fff; /* 文字颜色 */\n  margin-left: 3%; /*根据需要调整左边距*/\n  top: -8px;\n}\n\n.header-one-image {\n  margin-bottom: 18px;\n  width: 60px;\n  height: 40px;\n  float: right;\n}\n\n.a-button-logIn,\n.a-button-help {\n  position: relative;\n  border: none;\n  background: #001529;\n  color: #fff;\n  box-shadow: none; /* 去除阴影 */\n  top: -6px;\n}\n\n.header-one-dropdown {\n  margin-left: 16px;\n  margin-bottom: 15%;\n}\n\n.content {\n  padding: 16px;\n}\n\n.header-tow {\n  background: #ededed;\n  padding: 0 16px;\n  height: 50px;\n}\n\n.header-tow-button {\n  margin-top: 8px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.header-tow-button .a-button-Map,\n.header-tow-button .a-button-Data,\n.header-tow-button .a-button-user {\n  margin-left: 10px;\n  margin-right: 10px;\n  position: relative;\n  border: none;\n  background: #ededed;\n  box-shadow: none; /* 去除阴影 */\n}\n\n.active-tab::after {\n  content: \"\";\n  display: block;\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  height: 2px;\n  background: #1890ff;\n}\n\n/* 弹窗样式 */\n.a-modal-button-one {\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n}\n.a-modal-button-tow {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n.a-model-button-logIn {\n  margin-left: 10px;\n  margin-right: 10px;\n  width: 300px;\n}\n.a-model-button-enroll {\n  margin-left: 10px;\n  margin-right: 10px;\n  border: none;\n  background: #fff;\n  box-shadow: none; /* 去除阴影 */\n}\n</style>\n"],"mappings":";AAkHA,SAASA,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,SAASC,YAAW,QAAS,uBAAuB;AACpD,SAASC,SAAQ,QAAS,YAAY;AACtC,SACEC,MAAM,EACNC,MAAM,EACNC,KAAK,EACLC,IAAI,EACJC,KAAK,EACLC,aAAa,EACbC,OAAO,QACF,gBAAgB;AAEvB,eAAe;EACbC,UAAU,EAAE;IACV,UAAU,EAAEP,MAAM;IAClB,iBAAiB,EAAEA,MAAM,CAACQ,MAAM;IAChC,kBAAkB,EAAER,MAAM,CAACS,OAAO;IAClC,iBAAiB,EAAET,MAAM,CAACU,MAAM;IAChC,OAAO,EAAEV,MAAM,CAACW,GAAG;IACnB,OAAO,EAAEX,MAAM,CAACY,GAAG;IACnB,UAAU,EAAEX,MAAM;IAClB,SAAS,EAAEC,KAAK;IAChB,aAAa,EAAEC,IAAI,CAACU,IAAI;IACxB,SAAS,EAAET,KAAK;IAChB,kBAAkB,EAAEC;EACtB,CAAC;EACDS,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,cAAc,EAAE,KAAK;MAAE;MACvBC,I