{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { Layout, Avatar, Dropdown, Menu } from \"ant-design-vue\";\n// Button,\n\nexport default {\n  components: {\n    \"a-layout\": Layout,\n    \"a-layout-header\": Layout.Header,\n    \"a-layout-content\": Layout.Content,\n    // \"a-layout-footer\": Layout.Footer,\n    \"a-row\": Layout.Row,\n    \"a-col\": Layout.Col,\n    // \"a-button\": Button,\n    \"a-avatar\": Avatar,\n    \"a-dropdown\": Dropdown,\n    \"a-menu\": Menu,\n    \"a-menu-item\": Menu.Item\n  },\n  methods: {\n    setActiveTab(tabIndex) {\n      this.position = tabIndex;\n      switch (tabIndex) {\n        case 1:\n          this.$router.push({\n            path: \"/\"\n          });\n          break;\n        case 2:\n          this.$router.push({\n            path: \"/data\"\n          });\n          break;\n        case 3:\n          this.$router.push({\n            path: \"/user\"\n          });\n          break;\n        default:\n          this.$router.push({\n            path: \"/\"\n          });\n      }\n    }\n  }\n};","map":{"version":3,"names":["Layout","Avatar","Dropdown","Menu","components","Header","Content","Row","Col","Item","methods","setActiveTab","tabIndex","position","$router","push","path"],"sources":["E:\\code\\Django\\project\\front\\front\\src\\components\\MainWindow.vue"],"sourcesContent":["<template>\n  <a-layout class=\"layout\">\n    <a-layout-header class=\"header-one\">\n      <a-row align=\"middle\" justify=\"space-between\" class=\"header-one-layout\">\n        <a-col :span=\"22\" class=\"header-one-imger\">\n          <img\n            src=\"../image/Snipaste_2024-12-13_12-10-58.png\"\n            alt=\"Logo\"\n            class=\"logo\"\n          />\n        </a-col>\n        <a-col :span=\"2\">\n          <!-- 用户头像和下拉菜单 -->\n          <a-dropdown class=\"header-one-dropdown\">\n            <a-avatar size=\"large\" icon=\"user\" />\n            <template #overlay>\n              <a-menu>\n                <a-menu-item key=\"user-center\">个人中心</a-menu-item>\n                <a-menu-item key=\"settings\">设置</a-menu-item>\n                <a-menu-item key=\"logout\">退出登录</a-menu-item>\n              </a-menu>\n            </template>\n          </a-dropdown>\n        </a-col>\n      </a-row>\n    </a-layout-header>\n\n    <a-layout-header class=\"header-tow\">\n      <a-row align=\"middle\" justify=\"space-between\" class=\"header-one-layout\">\n        <a-col :span=\"24\" class=\"header-tow-button\">\n          <a-button\n            key=\"1\"\n            @click=\"setActiveTab(1)\"\n            class=\"a-button-Map\"\n            :class=\"{ 'active-tab': activeTab === 1 }\"\n            >首页</a-button\n          >\n          <a-button\n            key=\"2\"\n            @click=\"setActiveTab(2)\"\n            class=\"a-button-Data\"\n            :class=\"{ 'active-tab': activeTab === 2 }\"\n            >产品</a-button\n          >\n          <a-button\n            key=\"3\"\n            @click=\"setActiveTab(3)\"\n            class=\"a-button-user\"\n            :class=\"{ 'active-tab': activeTab === 3 }\"\n            >关于我们</a-button\n          >\n        </a-col>\n      </a-row>\n    </a-layout-header>\n    <a-layout-content class=\"content\">\n      <router-view></router-view>\n    </a-layout-content>\n    <!-- <a-layout-footer class=\"footer\">\n\n      <div class=\"transparent-footer\">版权信息 © 2024</div>\n    </a-layout-footer> -->\n  </a-layout>\n</template>\n\n<script>\nimport { Layout, Avatar, Dropdown, Menu } from \"ant-design-vue\";\n// Button,\n\nexport default {\n  components: {\n    \"a-layout\": Layout,\n    \"a-layout-header\": Layout.Header,\n    \"a-layout-content\": Layout.Content,\n    // \"a-layout-footer\": Layout.Footer,\n    \"a-row\": Layout.Row,\n    \"a-col\": Layout.Col,\n    // \"a-button\": Button,\n    \"a-avatar\": Avatar,\n    \"a-dropdown\": Dropdown,\n    \"a-menu\": Menu,\n    \"a-menu-item\": Menu.Item,\n  },\n  methods: {\n    setActiveTab(tabIndex) {\n      this.position = tabIndex;\n      switch (tabIndex) {\n        case 1:\n          this.$router.push({ path: \"/\" });\n          break;\n        case 2:\n          this.$router.push({ path: \"/data\" });\n          break;\n        case 3:\n          this.$router.push({ path: \"/user\" });\n          break;\n        default:\n          this.$router.push({ path: \"/\" });\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.layout {\n  height: 100vh;\n  background: #f0f2f5;\n}\n\n.header-one {\n  background: #001529;\n  padding: 0 16px;\n  height: 50px;\n}\n\n.header-one-dropdown {\n  margin-left: 16px;\n  margin-bottom: 15%;\n}\n\n.content {\n  padding: 16px;\n}\n\n.logo {\n  height: 32px;\n  margin-right: 16px;\n}\n\n/* .footer {\n  height: 10px;\n  background: rgba(10, 10, 10, 0.4); \n  display: flex;\n  align-items: center;\n  justify-content: center;\n} */\n.header-tow {\n  background: #ededed;\n  padding: 0 16px;\n  height: 50px;\n}\n\n.header-tow-button {\n  margin-top: 10px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.header-tow-button .a-button-Map {\n  margin-left: 10px;\n  margin-right: 10px;\n}\n.header-tow-button .a-button-Data {\n  margin-left: 10px;\n  margin-right: 10px;\n}\n.header-tow-button .a-button-user {\n  margin-left: 10px;\n  margin-right: 10px;\n}\n\n.active-tab::after {\n  content: \"\";\n  display: block;\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  height: 2px;\n  background: #1890ff;\n}\n</style>\n"],"mappings":";AAiEA,SAASA,MAAM,EAAEC,MAAM,EAAEC,QAAQ,EAAEC,IAAG,QAAS,gBAAgB;AAC/D;;AAEA,eAAe;EACbC,UAAU,EAAE;IACV,UAAU,EAAEJ,MAAM;IAClB,iBAAiB,EAAEA,MAAM,CAACK,MAAM;IAChC,kBAAkB,EAAEL,MAAM,CAACM,OAAO;IAClC;IACA,OAAO,EAAEN,MAAM,CAACO,GAAG;IACnB,OAAO,EAAEP,MAAM,CAACQ,GAAG;IACnB;IACA,UAAU,EAAEP,MAAM;IAClB,YAAY,EAAEC,QAAQ;IACtB,QAAQ,EAAEC,IAAI;IACd,aAAa,EAAEA,IAAI,CAACM;EACtB,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,QAAQ,EAAE;MACrB,IAAI,CAACC,QAAO,GAA