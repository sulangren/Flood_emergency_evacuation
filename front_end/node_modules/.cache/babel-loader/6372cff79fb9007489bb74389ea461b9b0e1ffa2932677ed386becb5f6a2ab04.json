em\r\n        label=\"密码\"\r\n        name=\"password\"\r\n        rules=\"[{ required: true, message: '请输入密码!' }]\"\r\n      >\r\n        <a-input-password v-model=\"form.password\" placeholder=\"请输入密码\" />\r\n      </a-form-item>\r\n      <a-form-item\r\n        label=\"确认密码\"\r\n        name=\"confirmPassword\"\r\n        rules=\"[{ required: true, message: '请确认密码!' }]\"\r\n      >\r\n        <a-input-password\r\n          v-model=\"form.confirmPassword\"\r\n          placeholder=\"请确认密码\"\r\n        />\r\n      </a-form-item>\r\n      <a-form-item label=\"员工密钥（可选）\">\r\n        <a-input\r\n          v-model=\"form.employeeKey\"\r\n          placeholder=\"请输入员工密钥（如果适用）\"\r\n        />\r\n      </a-form-item>\r\n      <a-form-item label=\"手机号（可选）\">\r\n        <a-input v-model=\"form.phone\" placeholder=\"请输入手机号（如果适用）\" />\r\n      </a-form-item>\r\n      <a-form-item\r\n        :wrapper-col=\"{ offset: 8, span: 16 }\"\r\n        style=\"text-align: right\"\r\n      >\r\n        <a-button style=\"margin-right: 8px\" @click=\"handleCancel\"\r\n          >取消注册</a-button\r\n        >\r\n        <a-button type=\"primary\" native-type=\"submit\">提交</a-button>\r\n      </a-form-item>\r\n    </a-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent, reactive } from \"vue\";\r\nimport { Form, Input, InputPassword, Button } from \"ant-design-vue\";\r\n\r\nexport default defineComponent({\r\n  components: {\r\n    \"a-form\": Form,\r\n    \"a-input\": Input,\r\n    \"a-input-password\": InputPassword,\r\n    \"a-button\": Button,\r\n  },\r\n  setup() {\r\n    const form = reactive({\r\n      username: \"\",\r\n      password: \"\",\r\n      confirmPassword: \"\",\r\n      employeeKey: \"\",\r\n      phone: \"\",\r\n    });\r\n\r\n    const handleRegister = (values) => {\r\n      console.log(\"Register form submitted:\", values);\r\n      // 可以在这里添加验证逻辑和表单提交逻辑\r\n    };\r\n\r\n    const handleRegisterFailed = (errorInfo) => {\r\n      console.log(\"Failed:\", errorInfo);\r\n      // 可以在这里处理表单验证失败的逻辑\r\n    };\r\n\r\n    const handleCancel = () => {\r\n      console.log(\"Registration cancelled\");\r\n      // 可以在这里添加取消注册的逻辑\r\n    };\r\n\r\n    return {\r\n      form,\r\n      handleRegister,\r\n      handleRegisterFailed,\r\n      handleCancel,\r\n    };\r\n  },\r\n});\r\n</script>\r\n\r\n<style>\r\n.register-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-end;\r\n  padding-top: 20px;\r\n}\r\n\r\n.register-title {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n  margin-right: 20px; /* 确保标题靠右 */\r\n}\r\n\r\n/* 覆盖Ant Design Vue的默认样式，去除表单边框 */\r\n.ant-form {\r\n  border: none;\r\n  box-shadow: none;\r\n  width: 100%; /* 确保表单宽度 */\r\n}\r\n\r\n/* 调整表单项的布局 */\r\n.ant-form-item {\r\n  width: 100%; /* 确保表单项宽度 */\r\n}\r\n</style>\r\n"],"mappings":"AA0DA,SAASA,eAAe,EAAEC,QAAO,QAAS,KAAK;AAC/C,SAASC,IAAI,EAAEC,KAAK,EAAEC,aAAa,EAAEC,MAAK,QAAS,gBAAgB;AAEnE,eAAeL,eAAe,CAAC;EAC7BM,UAAU,EAAE;IACV,QAAQ,EAAEJ,IAAI;IACd,SAAS,EAAEC,KAAK;IAChB,kBAAkB,EAAEC,aAAa;IACjC,UAAU,EAAEC;EACd,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIP,QAAQ,CAAC;MACpBQ,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE;IACT,CAAC,CAAC;IAEF,MAAMC,cAAa,GAAKC,MAAM,IAAK;MACjCC,OAAO,CAACC,GAAG,CAAC,0BAA0B,EAAEF,MAAM,CAAC;MAC/C;IACF,CAAC;IAED,MAAMG,oBAAmB,GAAKC,SAAS,IAAK;MAC1CH,OAAO,CAACC,GAAG,CAAC,SAAS,EAAEE,SAAS,CAAC;MACjC;IACF,CAAC;IAED,MAAMC,YAAW,GAAIA,CAAA,KAAM;MACzBJ,OAAO,CAACC,GAAG,CAAC,wBAAwB,CAAC;MACrC;IACF,CAAC;IAED,OAAO;MACLT,IAAI;MACJM,cAAc;MACdI,oBAAoB;MACpBE;IACF,CAAC;EACH;AACF,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}